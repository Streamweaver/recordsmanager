@model IEnumerable<FireRosterMVC.Models.Codes.CareerDevelopmentLevel>

@{
    ViewBag.Title = "Career Development Levels";
}

<h2>Career Development Levels</h2>

<p>
    @Html.ActionLink("+ Add new", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Label)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Label, "Details", new { id = item.ID })
        </td>
        <td>
            @Html.Partial("_ListActionsPartial", new ViewDataDictionary { { "controller", "CareerDevelopmentLevel" }, { "id", item.ID } })
        </td>
    </tr>
}

</table>
