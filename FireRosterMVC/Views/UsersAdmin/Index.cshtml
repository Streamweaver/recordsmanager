@model IEnumerable<FireRosterMVC.Models.ApplicationUser>

@{
    ViewBag.Title = "Users List";
}

<h2>Users List</h2>

<p>
    @Html.ActionLink("+ Add User", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.UserName, "Details", new { id = item.Id })
            </td>
            <td>
                @Html.Partial("_ListActionsPartial", new ViewDataDictionary { { "controller", "UsersAdmin" }, { "id", item.Id } })
            </td>
        </tr>
    }

</table>
